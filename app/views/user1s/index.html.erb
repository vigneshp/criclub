<h1>Listing user1s</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Access token</th>
    <th>Extra1</th>
    <th>Extra2</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user1s.each do |user1| %>
  <tr>
    <td><%= user1.name %></td>
    <td><%= user1.email %></td>
    <td><%= user1.access_token %></td>
    <td><%= user1.extra1 %></td>
    <td><%= user1.extra2 %></td>
    <td><%= link_to 'Show', user1 %></td>
    <td><%= link_to 'Edit', edit_user1_path(user1) %></td>
    <td><%= link_to 'Destroy', user1, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User1', new_user1_path %>
